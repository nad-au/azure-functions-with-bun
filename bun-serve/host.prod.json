{
    "version": "2.0",
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    },
    "extensions": {
        "http": {
            "routePrefix": "api"
        }
    },
    "logging": {
        "logLevel": {
            "default": "Information",
            "Host.Aggregator": "Warning",
            "Host.Results": "Warning",
            "Function": "Warning",
            "Worker": "Information"
        }
    },
    "customHandler": {
        "description": {
            "defaultExecutablePath": "app"
        },
        "enableForwardingHttpRequest": false
    }
}